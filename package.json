  },
  "homepage": "https://github.com/sheron184/straydog-js#readme",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.3",
    "@types/node": "^22.15.17",
    "cpx": "^1.5.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.8.3"
  },
