{
  "name": "straydog-js",
  "version": "1.0.0-beta.2",
  "description": "Drop-in API monitoring for any Node.js backend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc & npm run copy-views",
    "dev": "tsc --watch",
